                    if (NOT PlayPASound) then
                          PlayPASound = TRUE;
                    endif;
               endif;
          endif;
     else
          if ((PA1wTripTime <> -1.0) AND (gametime - PA1wTripTime < 12.0)) then
               PA1wTripTime = -1.0;
          endif;
     endif;
endif;

