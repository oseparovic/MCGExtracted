if (isAlive(PA1w)) then
     getObjectPosition(PA1w,aPoint);
     if (inHotSpot(PLAYER_FORCE,aPoint[0],aPoint[1],100)) then
          PerimeterBreach = TRUE;
          SetRevealed(1,10.0,aPoint);
          if (PA1wTripTime == -1.0) then
               PA1wTripTime = gametime;
               if (NOT PlayPASound) then
                     PlayPASound = TRUE;
               endif;
          else
               if (gametime - PA1wTripTime >= 12.0) then
                    SetRevealed(2,100.0,aPoint);
